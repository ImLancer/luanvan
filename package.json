{
  "name": "book-store",
  "version": "1.0.0",
  "description": "Book-Store Rest Api",
  "main": "index.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --runInBand",
    "test-user": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose user.spec.js",
    "test-account": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose account.spec.js",
    "test-product": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose product.spec.js",
    "test-order": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose order.spec.js",
    "start": "nodemon start.js"
  },
  "jest": {
    "transform": {},
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs"
    ],
    "verbose": true,
    "coverageDirectory": "/home/abc/jest-supertest/coverage/my_reports/",
    "coverageReporters": [
      "html",
      "text"
    ]
  },
